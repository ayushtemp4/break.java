class arush { 
    public static void main(String[] args) 
    { 
        for (int i = 0; i < 10; i++) { 
            if (i == 5) 
                break; 
            System.out.println("i: " + i); 
        } 
        System.out.println("Out of Loop"); 
    } 
}
//output
i: 0
i: 1
i: 2
i: 3
i: 4
Out of Loop
